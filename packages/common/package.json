      "default": "./src/logger/index.ts"
    }
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "pino": "^9.8.0",
    "pino-pretty": "^13.1.1",
    "@sinclair/typebox": "^0.34.0"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
