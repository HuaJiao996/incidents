{
  "name": "@incidents/common",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./model": {
      "types": "./src/model/index.ts",
      "default": "./src/model/index.ts"
    },
    "./rengine": {
      "types": "./src/rengine/index.ts", 
      "default": "./src/rengine/index.ts"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "default": "./src/utils/index.ts"
    },
    "./validators/*": {
